## MongoDB connection string
# 로컬 개발(권장): 아래 기본값을 그대로 써도 됩니다.
# MONGODB_URI=mongodb://127.0.0.1:27017/compiling-project
#
# Atlas 사용 시 예시(값은 본인 것으로 교체):
# MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-host>/<db>?retryWrites=true&w=majority

## Session/cookie secret (필수: 운영에서는 강하게)
COOKIE_SECRET=change-me-in-production

## Frontend dev server origin (CORS)
FRONTEND_ORIGIN=http://localhost:5173

## Cookie sameSite (optional)
# 운영에서 프론트/백이 다른 도메인이면 none 권장 (secure=true 필요)
# COOKIE_SAMESITE=none

## Optional: fix SRV lookup failures in some networks
# DNS_SERVERS=8.8.8.8,1.1.1.1

## Server port (optional)
PORT=4000
MONGODB_URI=mongodb://127.0.0.1:27017/compiling-project
COOKIE_SECRET=dev-secret-change-in-production
FRONTEND_ORIGIN=http://localhost:5173

